{
  "database": "Zotero",
  "noteImportFolder": "source-notes",
  "pdfExportImageDPI": 120,
  "pdfExportImageFormat": "jpg",
  "pdfExportImageQuality": 90,
  "citeFormats": [
    {
      "name": "ACM SIGGRAPH",
      "format": "formatted-bibliography",
      "cslStyle": "acm-siggraph"
    }
  ],
  "exportFormats": [
    {
      "name": "source-notes",
      "outputPathTemplate": "source-notes/{{citekey}}.md",
      "imageOutputPathTemplate": "source-notes/{{citekey}}/",
      "imageBaseNameTemplate": "image",
      "templatePath": "Templates/source-notes.md",
      "cslStyle": "acm-siggraph"
    }
  ],
  "citeSuggestTemplate": "[[{{citekey}}]]",
  "openNoteAfterImport": false,
  "whichNotesToOpenAfterImport": "first-imported-note"
}